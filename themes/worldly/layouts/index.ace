= doctype html
html lang=en-US prefix="og: http://ogp.me/ns#"
  {{ partial "head" . }}
  body
    .wrapper-all
      {{ partial "header" . }}

      .hero
        a.hero__read-more-arrows href="#"

      .content
        ul.posts
          {{ range .Data.Pages }}
          li
            a href={{ .Permalink }}
              {{ .Title }}
          {{ end }}

    {{ partial "scripts" . }}
